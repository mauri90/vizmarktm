			/* We set up the variables that will be displayed or manipulated during the visualization here */
			var inputData = collegeData2001;
			var width = 800;
			var height = 600;
			var radius = 8;
			var toppad = 100
			var rightpad = 20;
			var bottompad = 40;
			var leftpad = 80;
			var shiftTextDown = 20;
			var INDUSTRY = 0;
			var STARTINGSALARY = 1;
			var MEDIANSALARY = 2;
			var GDP = 3;
			var DEGREES = 4;
			var yColumn = d3.select("#y-axis1").node().value;
			var xColumn = "Degrees";
			var rColumn = "StartingSalary";
			var nameColumn = "Industry";
			var colorColumn = "Industry";
			
			/* We declare the color map that will be used in differentiating between the categorical variables */
			var dotColor = d3.scale.category20();


			/* This will be used to calculate the maximum and minimum values for our axis */
			var xScale = d3.scale.linear()
				.domain([0, d3.max(inputData, function(d){return d[xColumn];})])
				.range([0, width - (leftpad + rightpad)]);
			var yScale = d3.scale.linear()
				.domain([0, d3.max(inputData, function(d){return d[yColumn];})])
				.range([height - (toppad + bottompad), 10]);

			var xAxis = d3.svg.axis()
				.scale(xScale)
				.orient("bottom")
				.ticks(3);
			var yAxis = d3.svg.axis()
				.scale(yScale)
				.orient("left")
				.ticks(5);
			var rMin = d3.min(inputData, function(d){return d[rColumn];});
			var rMax = d3.max(inputData, function(d){return d[rColumn];});
			var rScale = d3.scale.linear().range([rMin, rMax]);
			rScale.domain(d3.extent(inputData, function (d){return d[rColumn];}));
			

			/* We describe the function that will be used to draw our scatter plot. We also introduce all the relevant
			styles here that the user will perceive */
			function drawScatterGraph(data){
				
				var tip = d3.tip()
				  .attr('class', 'd3-tip')
				  .offset([0, 10])
				  .html(function(d) {
					return "<strong>Industry:</strong> <span style='color:red'>" + d["Industry"] + "</span><br><strong>GDP:</strong> <span style='color:red'>" + d["GDP"] + "</span><br><strong>Number of Graduates:</strong> <span style='color:red'>" + d["Degrees"] + "</span><br><strong>Starting Salary:</strong> <span style='color:red'>" + d["StartingSalary"] + "</span><br><strong>Mid-Career Salary:</strong> <span style='color:red'>" + d["MedianSalary"] + "</span>";
				  })
				
				/* We declare the height and width of the vizualization space */
				var svg = d3.select("#area2").append("svg")
					.attr("width", width)
					.attr("height", height);
				
				/* We declare the circles that will act as our scatter plot points in our visualization */
				var circles = svg.selectAll("circle")
					.data(data)
					.enter()
					.append("circle");
				
				/*We include the labeling for the circles as we intend for users to be able to hover over a data point 
				and retrieve more detailed information about that point of interest */
				circles.attr("cx", function(d){return xScale(d[xColumn]);})
					.attr("cy", function(d){return yScale(d[yColumn]);})
					.attr("r", function(d){return Math.log(rScale(d[rColumn]))})
					.attr("name", function(d){return d[nameColumn];})
					.attr("fill", function(d){return dotColor(d[colorColumn]);})
					.attr("transform", "translate("+leftpad+", "+(toppad)+")")
					.on("mouseover", tip.show)
					.on("mouseout", tip.hide);
					
				circles.call(tip);
				
				
				svg.append("g")
					.attr("class", "axis")
					.attr("transform", "translate("+leftpad+", "+(height-bottompad)+")")
					.call(xAxis);
				svg.append("g")
					.attr("class", "axis")
					.attr("transform", "translate("+leftpad+", "+(toppad)+")")
					.call(yAxis);
					
					//circles.exit().remove();
					
			}
			
			/* We create a function for a 'years' slider that animates through our multidimensional space. 
			The user can then observe 3 dimensions interact while viewing the interaction through time */
			function createSlider(){
				var width = 450;
				var height = 300;
				
				
				
				/*var years = ["2001", "2002", "2003", "2004", "2005", "2006", "2007", "2008", "2009", "2010", "2012", "2013", "2014", "2015"];*/
				var years = [2001, 2002, 2003, 2004, 2005, 2006, 2007, 2008, 2009, 2010, 2012, 2013, 2014, 2015];
				var sliderScale = d3.scale.linear()
					.domain([d3.min(years, function(d){return d;}), d3.max(years, function(d){return d;})])
					.range([0, width])
					.clamp(true);
				var reverseYear = d3.scale.linear()
					.domain([0, 400])
					.range([2001, 2015]);
				
				
				/* We create the slider here */
				var svg1 = d3.selectAll("#rightFooter").append("svg1")
					.attr("width", 600)
					.attr("height", 200);
				svg1.append("br");
				svg1.append("label")
					.attr("text", "Year: ");
				svg1.append("input")
					.attr("type", "range")
					.attr("class", "slider")
					.attr("min", 0)
					.attr("max", 400)
					.attr("id", "slider")
					.attr("width", "45")
					.attr("transform", "translate(40, 60)");
					//.call(sliderAxis);
					//svg.append("br");
				var svg2 = d3.selectAll("#rightYear").append("svg2")
					.attr("width", 600)
					.attr("height", 60);
				var textYear = svg2.append("text")
					.attr("stroke", "black")
					.text("2008")
					.attr("x", 80)
					.attr("y", 60)
					.attr("width", "20px")
					.attr("fill", "black")
					.attr("class", "yearLabel")
					.attr("text-align", "center")
					;
					
					//svg1.append("br");
					//
					//svg.append("g")
					//	.attr("class", "sliderAxis")
					//	.attr("transform", "translate(40, 60)");
						//.call(sliderAxis);
						
					d3.select("#slider")
						.on("input", function(){
							console.log("slider moved to: "+this.value); 
							textYear.text(Math.round(reverseYear(this.value)));
							//inputData = "collegeData"+Math.round(reverseYear(this.value));
							//drawScatterGraph();
							});
			}
			
			drawScatterGraph(inputData);
			//drawScatterGraph(inputData);
			createSlider();
		</script>
	
	</body>
</html>
